<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.swiftpot.android.mildred.fragments.FragmentHome">


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/imageButton"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_alignBottom="@+id/view6"
        android:layout_toStartOf="@+id/view6"
        app:backgroundTint="#a51818"
        android:src="@mipmap/ic_sign_in" />


    <view
        class="android.support.v4.widget.Space"
        id="@+id/view5"
        layout_centerHorizontal="true"
        android:layout_width="12dp"
        android:layout_height="48dp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/view6" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/imageButton2"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_alignBottom="@+id/view6"
        android:layout_toEndOf="@+id/view6"
        app:backgroundTint="@color/DeepGreen"
        android:src="@mipmap/ic_sign_out" />



</RelativeLayout>


